{
  "schemaVersion": 1,
  "name": "LemonClient",
  "id": "meteor-client",
  "version": "${version}",
  "description": "LemonClient is an advanced Minecraft utility client for Fabric forked from Meteor Client.",
  "authors": [
    "Fin_LemonKee - Owner",
    "ImWuMie - Contributor",
    "Fra5k_ - Logo Editor",
    "Minegame159 - Making Orbit"
  ],
  "contact": {
    "homepage": "https://lemonclient.com",
    "issues": "https://github.com/LemonClientDevelopment/lemon-client/issues",
    "sources": "https://github.com/LemonClientDevelopment/lemon-client",
    "discord": "https://jq.qq.com/?_wv=1027&k=G6lGuwCr"
  },
  "license": "GPL-3.0",
  "icon": "assets/meteor-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "meteordevelopment.meteorclient.LemonClient"
    ]
  },
  "mixins": [
    "meteor-client.mixins.json",
    "meteor-client-baritone.mixins.json",
    "meteor-client-indigo.mixins.json",
    "meteor-client-sodium.mixins.json",
    "meteor-client-canvas.mixins.json"
  ],
  "accessWidener": "meteor-client.accesswidener",
  "custom": {
    "meteor-client:devbuild": "${devbuild}",
    "meteor-client:color": "0,155,255",
    "meteor-client:commit": "${commit}",
    "modmenu": {
      "links": {
        "modmenu.discord": "https://meteorclient.com/discord"
      }
    }
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19.1",
    "fabricloader": ">=0.14.8"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*",
    "wurst": "*"
  }
}
